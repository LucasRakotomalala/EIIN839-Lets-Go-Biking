<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="SOAPEndPoint" maxReceivedMessageSize = "1000000000"/>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/JCDecaux/" binding="basicHttpBinding"
                bindingConfiguration="SOAPEndPoint" contract="Routing.IRouting"
                name="SOAPEndPoint" />
        </client>
    </system.serviceModel>
</configuration>